a, b, c, d = map(int, input().split())

for i in range(b * d + 1):
    if i % b == 0 and i % d == 0:
        if i != 0:
            # print (i)
            break
a = (i // b) * a
c = (i // d) * c
q = a + c
# print (q)
j = 1
for j in range(q + i):
    if j == 0:
        r = 1
    else:
        if q % j == 0 and i % j == 0:
            f = q // j
            h = i // j

if i == q:
    print(1, 1)
else:
    if f != 0 and h != 0:
        print(f, h)
    else:
        print(q, i)

